#define _POSIX_C_SOURCE 200809L
#include <stdio.h>
#include <stdlib.h>
#include <string.h>

int main() {
  char *user_input = NULL;
  size_t len = 0;
  char *saveptr = NULL;
  getline(&user_input,&len,stdin);
  char *token = strtok_r(user_input, "/", &saveptr);
  while (token != NULL) {
    token = strtok_r(NULL, "/", &saveptr);
  }
  
  free(user_input);
}
